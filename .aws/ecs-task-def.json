{
    "family": "github-actions-task-def-family",
    "networkMode": "awsvpc",
    "containerDefinitions": [{
        "name": "github-actions",
        "image": "385320741566.dkr.ecr.us-east-1.amazonaws.com/github-actions-web-1:latest",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80
        }],
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/github-actions-log-group",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::385320741566:role/ecsTaskExecutionRoleNoorGitHub",
    "taskRoleArn": "arn:aws:iam::385320741566:role/ecsTaskRoleNoorGitHub"
}
